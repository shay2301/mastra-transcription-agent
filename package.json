{
  "name": "mastra-live-transcriber",
  "version": "1.0.0",
  "description": "Real-time transcription agent using Mastra, Whisper via Groq/OpenRouter",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "demo": "tsx scripts/demo.ts",
    "discord:register": "tsx scripts/register-discord-commands.ts",
